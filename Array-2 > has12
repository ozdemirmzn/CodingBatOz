//Given an array of ints, return true if there is a 1 in the array with a 2 somewhere later in the array.


public boolean has12(int[] nums) {
  int index=0;
  boolean check=false;
  for(int i=0; i<nums.length; i++){
    if(nums[i]==1){
      index=i;
      for(int j=index; j<nums.length; j++){
        if(nums[j]==2){
          check=true;
          break;
        }
    
  }
    }
  }return check;
  
}
